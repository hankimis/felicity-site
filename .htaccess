RewriteEngine On

# 1. index.html → / 로 301 리다이렉트 (우선 순위 높임)
RewriteCond %{THE_REQUEST} \s/+index\.html[\s?] [NC]
RewriteRule ^index\.html$ https://www.onbitlabs.com/ [R=301,L]

# 2. HTTP 또는 non-www 접근 시 https://www.onbitlabs.com 로 통일
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} ^onbitlabs\.com$ [NC]
RewriteRule ^(.*)$ https://www.onbitlabs.com/$1 [R=301,L]