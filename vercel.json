{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "onbitlabs.com"
        }
      ],
      "destination": "https://www.onbitlabs.com/:path*",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(Mobile|Mobi|Android|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini).*"
        }
      ],
      "destination": "/community/",
      "permanent": false
    },
    {
      "source": "/embed/community",
      "destination": "/embed/community.html",
      "permanent": true
    },
    {
      "source": "/index.html",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(Mobile|Mobi|Android|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini).*"
        }
      ],
      "destination": "/community/",
      "permanent": false
    }
  ],
  "rewrites": [
    { "source": "/search", "destination": "/search/index.html" },
    { "source": "/search/(.*)", "destination": "/search/$1" },
    { "source": "/api/ai-search", "destination": "/api/ai-search" },
    { "source": "/feed", "destination": "/feed/index.html" },
    { "source": "/feed/", "destination": "/feed/index.html" },
    { "source": "/feed/write", "destination": "/feed/write.html" },
    { "source": "/feed/post/:id", "destination": "/feed/post.html" },
    { "source": "/feed/topic/:slug", "destination": "/feed/topic.html" },
    { "source": "/feed/me", "destination": "/feed/index.html" },
    { "source": "/feed/notifications", "destination": "/feed/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self';"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/embed/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self';"
        }
      ]
    },
    {
      "source": "/charting_library-master/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self';"
        }
      ]
    }
    
  ]
}